    "version": "0.2.0",
    "configurations": [{
            "name": "Python: Django",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "runserver",
                "0.0.0.0:7002",
                "--settings=config.debug.settings",
            ],
            "django": true
        },
        {
            "name": "Python: Django Notebook",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "shell_plus",
                "--notebook"
            ],
            "django": true
        },
        {
            "name": "Python: Django Test",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "test",
                "--no-input"
            ],
            "django": true
        },
        {
            "name": "Python: Without DB Django Test",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "test",
                "--keepdb",
                "--no-input"
            ],
            "django": true
        }
    ]
}